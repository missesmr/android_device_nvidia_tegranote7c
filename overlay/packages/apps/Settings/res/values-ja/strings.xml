<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Moving app to SD card -->
    <add-resource type="string" name="move_app_app_label" />
    <string name="move_app_app_label">アプリ</string>
    <add-resource type="string" name="move_app_data_label" />
    <string name="move_app_data_label">データ</string>
    <add-resource type="string" name="move_app_to_device_dlg_title" />
    <string name="move_app_to_device_dlg_title">アプリをデバイスに移動しますか?</string>
    <add-resource type="string" name="move_app_to_sdcard_dlg_title" />
    <string name="move_app_to_sdcard_dlg_title">アプリを SD カードに移動しますか?</string>
    <add-resource type="string" name="move_app_dlg_text" />
    <string name="move_app_dlg_text">これには数分間かかることがあります。操作の途中で SD カードを取り外さないでください。</string>
    <add-resource type="string" name="move_app_free_on_device" />
    <string name="move_app_free_on_device"><xliff:g id="free_storage">%1$s</xliff:g> の空き容量がデバイスにあります</string>
    <add-resource type="string" name="move_app_free_on_sdcard" />
    <string name="move_app_free_on_sdcard"><xliff:g id="free_storage">%1$s</xliff:g> の空き容量が SD カードにあります</string>
    <add-resource type="string" name="move_app_progress_to_device" />
    <string name="move_app_progress_to_device">デバイスに <xliff:g id="pkg_label">%1$s</xliff:g> を移動しています...</string>
    <add-resource type="string" name="move_app_progress_to_sdcard" />
    <string name="move_app_progress_to_sdcard">SD カードに <xliff:g id="pkg_label">%1$s</xliff:g> を移動しています...</string>
</resources>
